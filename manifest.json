{
  "manifest_version": 2,
  "name": "AddressFiller",
  "author": "Jan Dědeček",
  "version": "1.4.1",
  "description": "Address Filler makes it quick and easy to fill in an address form with the address at your current location. It uses your phone's GPS support to determine the current location.
",
  "icons": {
    "80": "images/icon.png"
  },
  "browser_action": {
    "default_icon": {
      "80": "images/icon.png"
    }
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "bower_components/jquery/jquery.min.js",
        "bower_components/sprintf/src/sprintf.min.js",
        "js/content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "permissions": [
    "http://*.googleapis.com/"
  ]
}
